binSearch(lis,first,last,val):

        first = 0

        middle = len(lis)//2

	last = len(lis)-1

        if (middle == target):
                print("found")

        elif (target > middle):
                binSearch(lis,middle+1,last,val) 

        else:
                binSearch(lis,first,middle-1,val)


binSearch([1,2,4,5,6,8,9],6)
